{
    "profiles": {
        "http": {
            "commandName": "Project",
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "dotnetRunMessages": true,
            "launchBrowser": true,
            "launchUrl": "scalar/v1",
            "applicationUrl": "http://localhost:5171"
        },
        "https": {
            "commandName": "Project",
            "launchBrowser": true,
            "launchUrl": "scalar/v1",
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "dotnetRunMessages": true,
            "applicationUrl": "https://localhost:7232;http://localhost:5171"
        },
        "Container (Dockerfile)": {
            "commandName": "Docker",
            "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/scalar/v1",
            "environmentVariables": {
                "ASPNETCORE_HTTP_PORTS": "80"
            },
            "publishAllPorts": false,
            "useSSL": false
        }
    },
    "$schema": "https://json.schemastore.org/launchsettings.json"
}